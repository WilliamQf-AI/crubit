// Mock of standard library <memory> header.

namespace std {
template <class T>
class unique_ptr {
public:
unique_ptr() {}
T &operator*() const;
};

template <class T, class... Args>
unique_ptr<T> make_unique(Args &&...args);
}  // namespace std
